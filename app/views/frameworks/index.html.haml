%h1= t('frameworks.index.tilte')
-unless @category.nil?
  .row
    .columns.small-12
      %h4= t('frameworks.index.category', category: @category.name)
      =link_to t('frameworks.index.clear_category'), root_path
%ul.small-block-grid-1.medium-block-grid-2
  =@frameworks.any? ? render(@frameworks) : t('frameworks.index.empy')
= paginate @frameworks